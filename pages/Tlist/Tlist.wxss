Page{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  background:rgba(245,245,245,1);
}

image{
  width: 100%;
  height: 100%;
}

.content{
   width: 100%;
   height: 100%;
}

.pre{
  position: relative;
  background: rgba(255,255,255,1);
  margin: 30rpx 5%;
  padding: 30rpx 25rpx;
  border-radius: 8rpx;
}

.line{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.title{
  font-size: 28rpx;
  color: rgba(51,51,51,1);
}

.value{
  padding-left: 20rpx;
  font-size: 28rpx;
  color: rgba(153,153,153,1);
}

.line2{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.div{
  padding-bottom: 15rpx;
}

.fill{
  height: 10rpx;
  width: 100%;
  border-top: 1rpx solid rgba(193,193,193,1);
  opacity:0.25;
  margin-top: 10rpx;
}

.tag{
  position: absolute;
  right: 20rpx;
  top: 0;
}

.tagDiv{
  position: relative;
  width: 71rpx;
  height: 45rpx;
}

.statusFont{
  position: absolute;
  font-size: 18rpx;
  color: white;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 42rpx;
}
