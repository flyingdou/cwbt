Page{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  background:rgba(245,245,245,1);
}

image{
  display: block;
  width: 100%;
  height: 100%;
}

.content{
  width: 100%;
  height: 100%;
  background: white;
  margin-top: 30rpx;
}

.line{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 4%;
  height: 80rpx;
  border-radius: 8rpx;
}

.title{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 100%;
}

.multiSelector{
  width: 50%;
  font-size: 28rpx;
}

.dept-block{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  height: 80rpx;
}

.dept-name{
  margin-right: 10rpx;
}

.moreIcon{
  margin-top: 6rpx;
  width: 26rpx;
  height: 26rpx;
}

.title{
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
}

.value{
  font-size: 28rpx;
  color: rgba(153, 153, 153, 1);
}

.fill{
  width: 100%;
  height: 30rpx;
  background:rgba(245,245,245,1);
}

.linex{
  margin-top: 50%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.button{
  color: white;
  padding: 70rpx 30rpx;
  font-size: 30rpx;
  border-radius: 8rpx;
}

.workCard{
  background: #27CDFF;
}

.maintance{
  background: #3290FF;
}


