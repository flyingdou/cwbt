Page{
  height: auto;
}

.content{
  position: relative;
}

.spareList{
  margin:20rpx;
}

.preSpare{
  background:white;
  padding:20rpx;
  border-radius:8rpx;
  margin-bottom: 20rpx;
}

.line{
  display:flex;
  justify-content:space-between;
  align-items:center;
  padding:10rpx 0;
}

.right{
  margin-top: 20rpx;
  padding-top: 10rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  border-top: 1rpx solid rgba(245,245,245,1);
}

.footer{
  position: fixed;
  left: 0;
  bottom: 20rpx;
  width: 100%;
}

.pick{
  font-size: 28rpx;
  color: white;
  text-align: center;
  padding: 18rpx 0;
  margin: 0 20rpx;
  background: #2682FF;
  border-radius: 5rpx;
}