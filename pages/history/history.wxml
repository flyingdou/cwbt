<view class=' content'>
      <!-- 选择查询时间 -->
      <view class='times'>
            <!-- startTime -->
            <view class='line' bindtap='chooseDate' data-key = 'startTime'>
                  <!-- tv -->
                  <view class='tv'>
                        <view class='title'>开始时间:</view>
                        <view class='value'>{{startTime}}</view>
                  </view>

                  <!-- moreIcon -->
                  <view class='moreIcon'>
                        <image src='../../icon/more@2x.png' mode='aspectFill'></image>
                  </view>

            </view>
            
            <!-- endTime -->
            <view class='line' bindtap='chooseDate' data-key = 'endTime'>
                  <!-- tv -->
                  <view class='tv'>
                        <view class='title'>结束时间:</view>
                        <view class='value'>{{endTime}}</view>
                  </view>

                  <!-- moreIcon -->
                  <view class='moreIcon'>
                        <image src='../../icon/more@2x.png' mode='aspectFill'></image>
                  </view>

            </view>
      </view>

      <!-- 结果 -->
      <view class='weui-cell-third'>
            <view class='tagTitle'>维保记录</view>
            <block wx:for="{{list}}" wx:key="">
                  <view class='weui-cell-list'>
                        <view class='weui-cell-circle'></view>  
                        <view class='weui-cell-line'>
                              <view class='weui-cell-time'><text class="text">{{item.finishtime}}</text></view>
                              <view class='weui-cell-name'></view>
                              <view class='weui-cell-event'>名称: <text class="text">{{item.name}}</text></view>
                              <view class='weui-cell-event'>执行人: <text class="text">{{item.executor}}</text></view>
                              <view class='weui-cell-event'>状态: <text class="text">{{item.statusName}}</text></view>
                        </view>
                  </view>
            </block>
      </view>
</view>

<!-- 日历组件 -->
<mark showModalStatus = '{{showModalStatus}}' bindMark = 'saveSelectValue'></mark>
