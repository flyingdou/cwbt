Page{
  margin: 0;
  padding: 0;
  width: 100%;
  height: auto;
  background: rgba(245,245,245,1);
}

.content{
  margin: 20rpx;
}

.weui-cell-third{  
  background: #fff;
}

.weui-cell-list{  
  background: #fff;  
  margin: 5px 50px 5px 50px;
}
.weui-cell-line{
  /* width: 100px; */
  margin-left: 5px;
  padding-bottom: 20rpx;  
  border-left: 1px solid #ddd;  
  background: #fff
}
.weui-cell-circle{  
  border: 1px solid #000;  
  border-radius: 5px;  
  width: 10px;  
  height: 10px;  
  border-color: blue;
}
.weui-cell-time{  
  /* width: 50px; */  
  float: left;  
  font-size:14px;  
  padding-left: 15px;  
}
.weui-cell-event{   
  padding-top: 8px;  
  padding-left: 15px;
  font-size: 28rpx;
}
.weui-cell-name{
  font-size:14px;
  margin-left: 100px;  
  width: 100%;
  height: 14px;
}
.text{
  color: rgba(153,153,153,1);
}


