.content{
  padding: 20rpx 0;
}

.weui-cell-third{  
  background: #fff;
  margin-top: 20rpx;
}

.weui-cell-list{  
  background: #fff;  
  margin: 5px 50px 5px 50px;
}
.weui-cell-line{
  margin-left: 5px;
  padding-bottom: 20rpx;  
  border-left: 1px solid #ddd;  
  background: #fff
}
.weui-cell-circle{  
  border: 1px solid #000;  
  border-radius: 5px;  
  width: 10px;  
  height: 10px;  
  border-color: blue;
}
.weui-cell-time{  
  float: left;  
  font-size:14px;  
  padding-left: 15px;  
}
.weui-cell-event{   
  padding-top: 8px;  
  padding-left: 15px;
  font-size: 28rpx;
}
.weui-cell-name{
  font-size:14px;
  margin-left: 100px;  
  width: 100%;
  height: 14px;
}
.text{
  color: rgba(153,153,153,1);
}

.times{
  padding: 0 20rpx;
  background: white;
}

.timeDiv{
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.middle{
  margin-left: 20rpx;
  font-size: 28rpx;
}

.timeDiv-timeright{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-left: 20rpx;
}

.title{
  padding-left: 20rpx;
  font-size: 24rpx;
  text-align: center;
}

.right{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  text-align: center;
}

.line{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 1rpx solid rgba(245,245,245,1);
}

.line:last-child{
  border-bottom: none;
}

.tv{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.value{
  padding-left: 20rpx;
}

.moreIcon{
  width: 26rpx;
  height: 26rpx;
}

.tagTitle{
   font-size: 28rpx;
   color: black;
   background: #ccc;
   padding: 20rpx 0;
   text-align: center;
}



