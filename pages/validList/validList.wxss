.content{
  position: relative;
}

.list{
  padding: 20rpx;
}

.pre{
  background: white;
  padding:20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-radius: 8rpx;
  margin-bottom: 30rpx;
}

.line1{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.lineFill{
  width: 100%;
  height: 2rpx;
  background: rgba(245,245,245,1);
  margin: 20rpx 0;
}

.line{
  width: 88%;
}

.line2{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.left{
 margin-right: 0rpx;
}

.tv{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.value{
  padding-left: 20rpx;
}

.footer{
  position: fixed;
  left: 0;
  bottom: 40rpx;
  width: 100%;
  height: auto;
}

.isAll {
  font-size: 28rpx;
  color: rgba(51,51,51,1);
  padding: 20rpx;
  text-align: right;
}

.buttons{
