Page{
  padding: 0;
  margin: 0;
  width: 100%;
  height: 100%;
  background: #f0f0f2;
}

.content{
  width: 100%;
  height: 100%;
}


/* tab css */
.tab_title{
  margin-top: 1rpx;
  display: flex;
  justify-content: space-around;
  background: white;
}

.tab_title_sub{
  font-size: 28rpx;
  color: #9f9f9f;
  text-align: center;
  padding: 20rpx 0;
}

.tab_title_sub_active{
  position: relative;
  color: #313131;
}

.activeLine{
  position: absolute;
  bottom: 0;
  left: 22%;
  width: 70rpx;
  height: 4rpx;
  background: #67dde0;
}


.line{
  width: auto;
  height: auto;
  margin: 20rpx;
  padding: 20rpx;
  background: white;
  border-radius: 10rpx;
}

.upper{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.next{
  padding-top: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.title{
  font-size: 28rpx;
}

.value{
  font-size: 24rpx;
  color: #999;
}