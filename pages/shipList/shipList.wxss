Page{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  background: rgba(245,245,245,1);
}


.content{
  width: 100%;
  height: 100%;
}

.chooseBoat{
  padding: 20rpx 20rpx;

}

.pre{
  padding: 20rpx;
  background: white;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}


.line{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 0;
  padding-right: 40rpx;
}

.title{
  padding-left: 20rpx;
  font-size: 28rpx;
  color: rgba(51,51,51,1);
}

.value{
  padding-left: 20rpx;
  font-size: 28rpx;
  color: rgba(153,153,153,1);
}