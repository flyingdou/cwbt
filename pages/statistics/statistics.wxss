Page{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  background: rgba(245,245,245,1);
}

image{
  display: block;
  width: 100%;
  height: 100%;
}

picker{
  padding-right: 30rpx;
}

.content{
  position: relative;
  width: 92%;
  height: 100%;
  margin: 0 4%;
  margin-top: 30rpx;
}

.line{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 4%;
}

.moreIcon{
  width: 26rpx;
  height: 26rpx;
}

.title{
  font-size: 28rpx;
  color: rgba(51,51,51,1);
}

.value{
  font-size: 28rpx;
  color: rgba(153, 153, 153, 1);
  padding-left: 20rpx;
}

.fill{
  width: 100%;
  height: 30rpx;
  background: rgba(245,245,245,1);
}

.pickers{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 40rpx;
  padding-top: 10rpx;
}


.choose{
  padding: 20rpx 4%;
  background: white;
  border-radius: 8rpx;
}

.valuess{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.lp{
  padding-right: 10rpx;
}

.footer{
  position: fixed;
  bottom: 40rpx;
  left: 0;
  width: 100%;
}

.sure{
  font-size:28rpx;
  padding:10rpx 20rpx;
  color:white;
  background:#27CDFF;
  text-align:center;
  margin:0 auto;
  width:15%;
  border-radius:5rpx;
}

.timeLine{
   padding-top: 20rpx;
}

.timeDiv{
  padding-bottom: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.right{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.statisticsList{
  margin-top:10px;
  background:white;
  border-radius:8rpx;

}

.pre{
   padding: 30rpx 20rpx;
   border-bottom: 1rpx solid rgba(245,245,245,1);
}

.preStatistics{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.preValid{
   display: flex;
   justify-content: space-between;
   align-items: center;
}

.pre:last-child{
  border: none;
}

.blocks{
  overflow: hidden;
  width: 90%;
}

.block{
  float: left;
  width: 30%;
  text-align: center;
}


.tv{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-bottom: 10rpx;
}

.lineBlocks{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.lineBlock{
  padding-right: 50rpx;
}
