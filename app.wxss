/**app.wxss**/
Page{
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  background:rgba(245,245,245,1);
}

.content{
  height: 100%;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

image{
  display: block;
  width: 100%;
  height: 100%;
}

.title{
  font-size: 28rpx;
  color: rgba(51,51,51,1);
}

.value{
  font-size: 28rpx;
  /* color: rgba(153,153,153,1); */
  color: rgba(128,128,128,1);
}

/* 返回上一级按钮样式 */
.backButtons{
  position: fixed;
  left: 0;
  bottom: 40rpx;
  width: 100%;
  display: flex;
}

.back{
  font-size: 28rpx;
  color: white;
  background: black;
  text-align: center;
  padding: 10rpx 20rpx;
  margin: 0 auto;
  border-radius: 10rpx;
}

